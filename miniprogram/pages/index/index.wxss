page{
  background-color: #d8e3e7;
  background-repeat: no-repeat;
}
.contain{
  display: flex;
  flex-direction: column;
}
.userinfo{
  display: flex;
  flex-direction: row;
  justify-content: center;
  background-color: #d8e3e7;
  margin-bottom: 40rpx;
  border: none;
  align-content: center;
  place-content: space-evenly;
}
.avatar{
  border-radius: 50%;
  /* margin: 20rpx; */
  height: 130rpx;
  width: 130rpx;
  justify-self: center;
  align-self: center;
  overflow: hidden;
}
.name{
  font-size: large;
  font-weight: bold;
  color: cadetblue;
}
.test{
  display: flex;
  flex-direction: column;
  /* justify-content: center;
  align-content: center; */
}
.pk{
    margin: 50rpx;
    text-align: center;
    font-weight: bold;
    font-size: x-large;
    color: #EFEFEF;
    background-image: linear-gradient(to bottom right, #78C4D4, #7868E6);    box-shadow: 19rpx 21rpx 28rpx #c5c5c5;
    border-radius: 10rpx;
    width: 400rpx;
    align-self: center;
}
.text{
  height: 100rpx;
  line-height: 100rpx;
}
.student{
  height: 130rpx;
  font-size: large;
  color: gray;
  display: flex;
  flex-direction: column;
  text-align: center;
  justify-content: center;
  align-items: flex-start;
}
.score{
  height: 130rpx;
  font-size: large;
  color: darkgreen;
  display: flex;
  flex-direction: column;
  text-align: center;
  align-items: flex-start;
}
.b1{
  margin: 50rpx;
  text-align: center;
  font-weight: bold;
  font-size: x-large;
  color: #EFEFEF;
  box-shadow: 19rpx 21rpx 28rpx #c5c5c5;
  border-radius: 10rpx;
  width: 400rpx;
  align-self: center;
  background-image: linear-gradient(to bottom right,#ffce86,#EEB76B,#efbe2d);
  /* animation-name: example;
  animation-duration: 4s;
  animation-iteration-count: infinite; */
}
.b2{
  margin: 50rpx;
  text-align: center;
  font-weight: bold;
  font-size: x-large;
  color: #EFEFEF;
  box-shadow: 19rpx 21rpx 28rpx #c5c5c5;
  border-radius: 10rpx;
  width: 400rpx;
  align-self: center;
  background-image: linear-gradient(to right bottom, #7868E6, #70416D);
}
.robot{
  width: 200rpx;
  height: 200rpx;
  margin-top: 40rpx;
  opacity: .55;
  align-self: center;
}
.bottom{
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  /* margin-top: 38px;wxcs_style_margin-top; */
  /* background: #f0f0f0; */
  padding: 8rpx;
  margin: 30rpx;
  /* box-shadow: 17rpx 12rpx 26rpx #c5c5c5; */
  border-radius: 8rpx;
  margin-top: 80rpx;
}
.rank{
  width: 60rpx;
  height: 60rpx;
  background-color: lavender;
  border-radius: 50%;
  padding: 12rpx;
}
.rankb{
  display: flex;
  align-items: center;
  width: 230rpx;
  justify-content: space-evenly;
}
.choices{
  display: flex;
  flex-direction: column;
}
@keyframes example {
  0%   {width: 400rpx;}
  25%   {width: 420rpx;}
  50%  {width: 450rpx;}
  75%   {width: 420rpx;}
  100% {width: 400rpx;}
}
.tipic{
  position: relative;
  opacity: 0.2;
  width: -webkit-fill-available;
  height: -webkit-fill-available;
}
.icon{
  height: 60rpx;
  width: 60rpx;
}
.line{
  padding: 0 20px 0;
  margin: 30px 0;
  line-height: 1px;
  /* border-left: 140px solid darkblue; */
  /* border-right: 140px solid darkblue; */
  text-align: center;
  color: darkblue;
  font-weight: 300;
  border-bottom: thin dotted;
  padding-bottom: 30rpx;
}
